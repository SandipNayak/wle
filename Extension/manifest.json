{

"name":"Price Notifier",
"description":"notifies price changes",
"version":"1.0",

"manifest_version":2,

"browser_action":{
"default_icon":"icon.png",
"default_popup":"popup.html"
},
    
"content_scripts": [{
"matches": ["*//www.amazon.in/*"],
"js": ["script.js"]

}],

"permissions":[
   "tabs",
   "storage"
]

}
